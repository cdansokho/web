.img {
    width: 64px;
    height: 74px;
}

.main {
    text-rendering: optimizeLegibility;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    line-height: 18px;
    padding: 90px 0 60px 0;
    font-family: 'Oxygen', sans-serif;
    font-size: 12px;
    color: #252525;
    font-weight: 400;
    width: 100%;
    display: inline-block;
    text-align: center;
}

.bloc {
    width: 1000px;
    padding-right: 15px;
    padding-left: 15px;
    margin-right: auto;
    margin-left: auto;
    display: inline-block;
}

.ligne {
    height: inherit;
    width: inherit;
    padding-right: 15px;
    padding-left: 15px;
    margin-right: -15px;
    margin-left: -15px;
}

.colonne {
    height: inherit;
    width: inherit;
    position: relative;
    min-height: 1px;
    padding-right: 15px;
    padding-left: 15px;
    margin-right: -15px;
    margin-left: -15px;
}

.carte-liste {
    width: inherit;
    height: 55%;
}

form {
    width: inherit;
    height: inherit;
    height: 100%;
}

.grande-table {
    height: inherit;
    width: inherit;
}

table {
    width: inherit;
    height: inherit;
    max-width: 100%;
    margin-bottom: 20px;
    background-color: transparent;
    border-spacing: 0;
    border-collapse: collapse;
    border-left: 5px solid #f0f0f0;
    border-right: 5px solid #f0f0f0;
}

thead {
    height: 15%;
}

tr {
    border-color: #47bac1;
    border-right-color: rgb(71, 186, 193);
    border-left-color: rgb(71, 186, 193);

}

button.close {
    -webkit-appearance: none;
    padding: 0;
    cursor: pointer;
    background: 0 0;
    border: 0;
}

.close {
    float: right;
    font-size: 21px;
    font-weight: 700;
    line-height: 1;
    color: #000;
    text-shadow: 0 1px 0 #fff;
    filter: alpha(opacity=20);
    opacity: .2;
}

.groupe-input {
    position: relative;
    display: table;
    border-collapse: separate;
    width: 60%;
    margin-bottom: 0;
}

.groupe-input input {
    font: normal normal normal 14px/1 FontAwesome, "Lato", sans-serif;
}

.groupe-input .element-form {
    position: relative;
    z-index: 2;
    float: left;
    width: 60%;
    margin-bottom: 0;
}

.element-form {
    display: block;
    width: 100%;
    height: 34px;
    font-size: 14px;
    line-height: 1.42857143;
    color: #555;
    background-color: #fff;
    background-image: none;
    border: 1px solid #ccc;
    border-radius: 4px;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    -webkit-transition: border-color ease-in-out .15s, -webkit-box-shadow ease-in-out .15s;
    -o-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}

.zone-coupon {
    height: 40px;
}

.ul {
    padding-left: 0;
    list-style: none;
    height: inherit;
}

.carte-liste .zone-submit {
    width: 930px;
    float: right;
    display: block;
    padding: 20px 30px;
    border-left: 5px solid #f0f0f0;
    border-right: 5px solid #f0f0f0;
    border-bottom: 3px solid #f0f0f0;
    margin-bottom: 100px;
}


.ul {
    margin: 0;
    padding: 0;
    font-size: 12px;
}

.boutton {
    font-weight: 400;
    font-size: 13px;
    text-decoration: none;
    letter-spacing: 0.5px;
    transition: 0.2s ease-in;
    border-radius: 0;
    padding: 9px 10px;
    font-size: 12px;
    padding: 8px 0;
    font-size: 15px;
    padding: 13px 0;
    white-space: nowrap;
    vertical-align: middle;
    display: inline-block;
    border-radius: 4px;
    text-transform: uppercase;
    text-align: center;
    font-size: 14px;
    padding: 9px 0;
    outline: 0 !important;
    transition: all 0.3s ease;
    margin-bottom: 0;
    touch-action: manipulation;
    cursor: pointer;
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
}

.groupe-input-bis {
    padding: 6px 12px;
    font-size: 14px;
    font-weight: 400;
    line-height: 1;
    color: #555;
    text-align: center;
    background-color: #eee;
    border: 1px solid #ccc;
    border-radius: 4px;
    text-decoration: none;
}

.groupe-input-bis,
.groupe-input-boutton {
    width: 1%;
    white-space: nowrap;
    vertical-align: middle;
}

button,
input,
select,
textarea {
    font-family: inherit;
    font-size: inherit;
    line-height: inherit
}

.carte-liste .grande-table .table {
    margin-bottom: 0;
}

.carte-liste .grande-table .table thead tr {
    border-left: 5px solid #47bac1;
    border-left-color: rgb(71, 186, 193);
    border-right: 5px solid #47bac1;
    border-right-color: rgb(71, 186, 193);
}

.carte-liste .grande-table .table thead tr th:first-child {
    display: table-cell;
}

.carte-liste .grande-table .table thead tr th,
.orderBox .grande-table .table thead tr th {
    background-color: #47bac1;
    padding: 0;
    padding-left: 0px;
    height: 55px;
    vertical-align: middle;
    text-align: left;
    font-size: 15px;
    color: #ffffff;
    padding-left: 20px;
    border-right: 1px solid rgba(255, 255, 255, 0.2);
    border-bottom: none;
    padding-right: 70px;
}

.carte-liste .grande-table .table tbody tr td:first-child {
    display: table-cell;
}

.carte-liste .grande-table .table tbody tr td {
    padding-left: 20px;
    padding: 10px;
}

.carte-liste .grande-table .table tbody tr td,
.orderBox .grande-table .table tbody tr td {
    vertical-align: middle;
    text-transform: uppercase;
    font-weight: 700;
    border-bottom: 3px solid #f0f0f0;
    border-top: none;
}

.carte-liste .grande-table .table tbody tr td .close,
.orderBox.myAddress .grande-table .table tbody tr td .close {
    margin-right: 5px;
    display: inline-block;
    float: left;
    font-size: 35px;
    margin-right: 30px;
    color: #47bac1;
    opacity: 1;
    line-height: 74px;
    font-weight: normal;
    transition: all 0.3s ease-in-out;
}

.carte-liste .grande-table .table tbody tr td input {
    width: 70px;
    height: 40px;
    text-align: center;
    line-height: 40px;
    color: #333333;
    font-weight: 700;
    border: 1px solid #e4e4e4;
}

.carte-liste .zone-coupon,
.carte-liste .zone-resultat {
    width: auto;
    display: block;
    padding: 20px 30px;
    border-left: 5px solid #f0f0f0;
    border-right: 5px solid #f0f0f0;
    border-bottom: 3px solid #f0f0f0;
}

.carte-liste .zone-coupon .groupe-input {
    margin-bottom: 0;
    float: left;
}

.carte-liste .zone-coupon .groupe-input .element-form {
    margin-right: 10px;
    margin-right: 10px;
    border-color: #e4e4e4;
}

.carte-liste .zone-coupon .groupe-input .element-form,
.carte-liste .zone-coupon .groupe-input .groupe-input-bis,
.carte-liste .zone-coupon .boutton {
    border-radius: 0;
    box-shadow: none;
    background-color: #ffffff;
    border: 2px solid #d0d0d0;
}

.carte-liste .zone-coupon .groupe-input .groupe-input-bis {
    width: 35%;
    transition: all 0.3s ease-in-out;
}

.carte-liste .zone-coupon .groupe-input .groupe-input-bis,
.carte-liste .zone-coupon .boutton {
    text-transform: uppercase;
    font-size: 13px;
    font-weight: 700;
}

.carte-liste .zone-coupon .boutton {
    width: 18%;
    display: inline-block;
    transition: all 0.3s ease-in-out;
    padding: 0;
    line-height: 36px;
    color: #252525;
    float: right;
}

.carte-liste .ligne.zone-resultat {
    margin: 0;
}


.carte-liste .zone-resultat .ul .li span {
    width: 108px;
    display: inline-block;
    color: #888888;
}

.carte-liste .zone-resultat .ul .li {
    font-weight: 700;
    text-align: right;
    margin: 10px 0;
}

.carte-liste .zone-submit .submit {
    width: 196px;
    height: 55px;
    border-radius: 0;
    border: none;
    background-color: #47bac1;
    color: #ffffff;
    padding: 0 20px;
    line-height: 55px;
    position: relative;
    text-align: center;
    font-weight: 700;
    float: right;
    font-size: 15px;
    transition: all 0.3s ease-in-out;
}

.ul .li {
    display: block;
}

a {
    transition: all 0.3s ease;
    color: rgba(255, 255, 255, 0.25);
    text-decoration: none !important;
}

.petite-colonne {
    width: 16.66666667%;
}

.qnt {
    border: solid #f0f0f0;
    text-align: center;
    width: 16.66666667%;
    padding: 10px 15px 10px 15px;
}

.moyenne-colonne {
    width: 33.33333333%;
}

.grande-colonne {
    height: inherit;
    margin-left: 66.66666667%;
    width: 33.33333333%;
}

.total-ttc {
    color: #47bac1;
    font-size: 18px;
}

.msgcoupon1 {
    font-size: 14px;
    margin-top: 10px;
    color: darkgoldenrod;
}

.msgcoupon2 {
    font-size: 14px;
    margin-top: 10px;
    color: red;
}

.msgcoupon3 {
    font-size: 14px;
    margin-top: 10px;
    color: #47bac1;
}